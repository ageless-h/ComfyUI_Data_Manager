{
  "name": "comfyui-data-manager",
  "version": "1.0.0",
  "description": "ComfyUI Data Manager - File management extension",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --run --coverage"
  },
  "keywords": [
    "comfyui",
    "extension",
    "file-manager"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "happy-dom": "^20.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.17"
  }
}
